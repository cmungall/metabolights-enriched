                    {
                        "comments": [],
                        "name": "Extraction",
                        "protocolType": {
                            "comments": [],
                            "annotationValue": "Extraction",
                            "termSource": {
                                "comments": [],
                                "name": "MTBLS",
                                "file": "https://www.ebi.ac.uk/metabolights/resources/Metabolights.owl",
