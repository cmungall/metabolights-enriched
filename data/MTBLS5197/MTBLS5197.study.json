                "protocols": [
                    {
                        "comments": [],
                        "name": "Sample collection",
                        "protocolType": {
                            "comments": [],
                            "annotationValue": "Sample collection",
                            "termSource": {
                                "comments": [],
                                "name": "",
                                "file": "http://purl.obolibrary.org/obo/ncit.owl",
