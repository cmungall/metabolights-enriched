                    {
                        "comments": [],
                        "name": "Data transformation",
                        "protocolType": {
                            "comments": [],
                            "annotationValue": "Data transformation",
                            "termSource": {
                                "comments": [],
                                "name": "MTBLS",
                                "file": "https://www.ebi.ac.uk/metabolights/resources/Metabolights.owl",
                                "version": "",
