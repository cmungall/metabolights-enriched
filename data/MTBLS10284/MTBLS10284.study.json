                "protocols": [
                    {
                        "comments": [],
                        "name": "Sample collection",
                        "protocolType": {
                            "comments": [],
                            "annotationValue": "Sample collection",
                            "termSource": null,
                            "termAccession": ""
                        },
                        "description": "<p>The culture medium from the cultured BMSCs (~10^7 cells per sample) was removed using pipette. Then the cells were washed with PBS under 37 \u00b0C and the PBS was removed.</p>",
                        "uri": "",
                        "version": "",
                        "parameters": [
