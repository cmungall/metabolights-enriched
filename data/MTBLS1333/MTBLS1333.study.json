                    {
                        "comments": [],
                        "name": "Flow Injection Analysis",
                        "protocolType": {
                            "comments": [],
                            "annotationValue": "Flow Injection Analysis",
                            "termSource": null,
                            "termAccession": ""
                        },
                        "description": "<p>Samples were infused via robotic nanoflow ion source TriVersa NanoMate (Advion BioSciences, Ithaca NY, USA) into the mass spectrometer using chips with spraying nozzles of 4.1 \u00b5m.</p>",
                        "uri": "",
                        "version": "",
                        "parameters": [
                            {
                                "comments": [],
                                "parameterName": {
                                    "comments": [],
                                    "annotationValue": "FIA Instrument",
                                    "termSource": null,
